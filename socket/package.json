{
  "name": "onad-banner-broadcasting-system",
  "version": "1.0.0",
  "description": "A program to emit to all client real time using socketio",
  "main": "dist/app.js",
  "author": "leejineun",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsc",
    "postbuild": "browserify ./dist/public/client.js ./dist/public/hiddenEventHandler.js ./dist/public/programIdentifier.js -o ./dist/public/bundle.js && cp -r ./views ./dist/views",
    "start:debug": "tsc -w",
    "start:production": "node ./dist/app.js"
  },
  "dependencies": {
    "@types/http-errors": "^1.6.3",
    "@types/jquery": "^3.3.33",
    "@types/mysql": "^2.15.9",
    "@types/node-schedule": "^1.3.0",
    "@types/socket.io": "^2.1.4",
    "@types/socket.io-client": "^1.4.32",
    "@types/winston": "^2.4.4",
    "dotenv": "^8.2.0",
    "mysql": "^2.18.1",
    "node-schedule": "^1.3.2",
    "socketio": "^1.0.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "devDependencies": {
    "browserify": "^16.5.1",
    "rimraf": "^3.0.2"
  }
}